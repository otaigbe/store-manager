  "scripts": {
    "test": "psql -U postgres -f ./dbscripts/CreateDb.sql &&  _mocha --recursive --compilers js:babel-core/register src/test/version1/  --timeout 10000 --exit",
    "cover": "psql -U postgres -f ./dbscripts/CreateDb.sql && nyc  _mocha --recursive --compilers js:babel-core/register src/test/version1/  --exit",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src --out-dir ./build",
    "build": "npm run clean && npm run build-babel",
    "dev": "nodemon --exec babel-node src/index.js --env",
    "devdb": "nodemon --exec babel-node src/routes/dbparams.js --env",
    "start": "npm run build && node ./build/index.js",
    "dbscript": "psql -U postgres -f ./src/dbUtils/dbscripts/CreateDb.sql",
    "tables-setup-heroku": "heroku pg:psql -f  ./dbscript/CreateDbHeroku.sql",
    "coveralls": "nyc report --reporter=text npm run cover | coveralls"
